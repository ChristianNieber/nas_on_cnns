<features> ::= <convolution>
<convolution> ::= layer:conv [num-filters,int,32,256] [filter-shape,int,2,5] [stride,int,1,3] [padding:same/valid] [act:linear/relu/elu/sigmoid] [bias:True/False]
<classification> ::= layer:fc [act:linear/relu/elu/sigmoid] [num-units,int,64,1024] [bias:True/False] [batch-normalization:True/False]
<output> ::= layer:fc num-units:10 bias:True